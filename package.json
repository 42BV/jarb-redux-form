      "tsx",
      "js",
      "node"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/tests/.*\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "./tests/setupTest.ts"
    ],
    "setupFilesAfterEnv": [
      "./tests/setupTest.ts"
    ]
